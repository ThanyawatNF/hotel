<inherit inherit="contact_list">
    <field name="categ_id" position="replace"/>
    <field name="country_id" position="replace"/>
    <field name="industry_id" position="replace"/>
    <field name="region_id" position="replace"/>
    <field name="business_area_id" position="replace"/>
    <field name="fleet_size_id" position="replace"/>

    <field name="type" position="after">
        <field name="phone"/>
    </field>
    <field name="phone" position="after">
        <field name="email"/>
    </field>
    <field name="email" position="after">
        <field name="website"/>
    </field>
</inherit>
